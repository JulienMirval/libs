{
  "name": "renovate-config-cozy-konnector",
  "version": "1.2.7",
  "main": "index.js",
  "license": "MIT",
  "renovate-config": {
    "default": {
      "extends": [
        "cozy",
        "schedule:weekly"
      ],
      "updateNotScheduled": true,
      "packageRules": [
        {
          "packagePatterns": [
            "^cozy-konnector-libs",
            "^cozy-jobs-cli"
          ],
          "schedule": "at any time",
          "automerge": true,
          "major": {
            "automerge": false
          }
        }
      ]
    }
  }
}
