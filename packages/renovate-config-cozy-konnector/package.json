{
  "name": "renovate-config-cozy-konnector",
  "version": "1.2.8",
  "main": "index.js",
  "license": "MIT",
  "renovate-config": {
    "default": {
      "extends": [
        "cozy",
        "schedule:weekly"
      ],
      "updateNotScheduled": true,
      "packageRules": [
        {
          "packagePatterns": [
            "copy-webpack-plugin",
            "eslint-config-cozy-app",
            "git-directory-deploy",
            "husky",
            "svgo",
            "webpack",
            "webpack-cli"
          ],
          "automerge": true,
          "major": {
            "automerge": false
          }
        },
        {
          "packagePatterns": [
            "cozy-konnector-libs",
            "cozy-jobs-cli",
            "cozy-app-publish",
            "eslint-config-cozy-app"
          ],
          "schedule": "at any time",
          "automerge": true,
          "major": {
            "automerge": false
          }
        }
      ]
    }
  }
}
